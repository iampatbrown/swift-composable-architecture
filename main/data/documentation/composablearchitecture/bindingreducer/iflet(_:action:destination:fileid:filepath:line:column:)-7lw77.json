perator:"}]},{"code":["@Reducer","struct Parent {","  @ObservableState","  struct State {","    @Presents var child: Child.State?","    \/\/ ...","  }","  enum Action {","    case child(PresentationAction<Child.Action>)","    \/\/ ...","  }","","  var body: some ReducerOf<Self> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .ifLet(\\.$child, action: \\.child) {","      Child()","    }","  }","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"text":"The ","type":"text"},{"code":"ifLet","type":"codeVoice"},{"type":"text","text":" operator does a number of things to make integrating parent and child features"},{"type":"text","text":" "},{"type":"text","text":"ergonomic and enforce correctness:"}]},{"items":[{"content":[{"inlineContent":[{"text":"It forces a specific order of operations for the child and parent features:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"When a ","type":"text"},{"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/PresentationAction\/dismiss","isActive":true},{"type":"text","text":" action is sent, it runs the parent feature"},{"type":"text","text":" "},{"text":"before the child state is ","type":"text"},{"code":"nil","type":"codeVoice"},{"type":"text","text":"’d out. This gives the parent feature an opportunity to"},{"type":"text","text":" "},{"type":"text","text":"inspect the child state one last time before the state is cleared."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"When a "},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/PresentationAction\/presented(_:)","isActive":true,"type":"reference"},{"text":" action is sent it runs the","type":"text"},{"type":"text","text":" "},{"text":"child first, and then the parent. If the order was reversed, then it would be possible","type":"text"},{"type":"text","text":" "},{"type":"text","text":"for the parent feature to "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" out the child state, in which case the child feature"},{"type":"text","text":" "},{"type":"text","text":"would not be able to react to that action. That can cause subtle bugs."}],"type":"paragraph"}]}],"type":"unorderedList"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"It automatically cancels all child effects when it detects the child’s state is ","type":"text"},{"code":"nil","type":"codeVoice"},{"type":"text","text":"’d"},{"type":"text","text":" "},{"type":"text","text":"out."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Automatically "},{"type":"codeVoice","code":"nil"},{"type":"text","text":"s out child state when an action is sent for alerts and confirmation"},{"text":" ","type":"text"},{"type":"text","text":"dialogs."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"It gives the child feature access to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DismissEffect","isActive":true},{"type":"text","text":" dependency, which allows the"},{"text":" ","type":"text"},{"text":"child feature to dismiss itself without communicating with the parent.","type":"text"}],"type":"paragraph"}]}],"type":"unorderedList"}],"kind":"content"}],"kind":"symbol","sections":[],"schemaVersion":{"minor":3,"major":0,"patch":0},"abstract":[{"text":"Embeds a child reducer in a parent domain that works on an optional property of parent state.","type":"text"}],"hierarchy":{"paths":[["doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/BindingReducer","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/BindingReducer\/Reducer-Implementations"],["doc:\/\/ComposableArch